<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
       
       <bean class="com.springConstructor.certifiicate" name="person2">
       <constructor-arg value="Android with Kotlin"/>
       </bean>
       
       <bean class="com.springConstructor.person" name="person">
       <constructor-arg value="Faiz_Khan"/>
       <constructor-arg value="11" type="int"/>
       <constructor-arg ref="person2" />
       </bean>
        <bean class="com.springConstructor.person" name="person1">
       <constructor-arg value="Faman Khan"/>
       <constructor-arg value="12" type="int"/>
       <constructor-arg ref="person2" />
       </bean>
       
        <bean class="com.springConstructor.Addition" name="add">
       <constructor-arg value="35"/>
       <constructor-arg value="36"/>
       </bean>
       <bean class="com.springConstructor.Addition" name="add1">
        <constructor-arg value="37" type="double"/>
       <constructor-arg value="38" type="double"/>
       </bean>
       <bean class="com.springConstructor.Addition" name="add2">
        <constructor-arg value="39" type="int"/>
       <constructor-arg value="40" type="int"/>
       </bean>
</beans>